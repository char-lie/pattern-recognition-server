{
  "name": "pattern-recognition-server",
  "version": "0.0.1",
  "description": "Server for pattern recognition computer practica",
  "scripts": {
    "build": "webpack --mode production",
    "test": "npm run test:dependencies && npm run test:lint && jest",
    "test:csslint": "stylelint src/styles/*.css",
    "test:dependencies": "npm run test:unused && ncu",
    "test:eslint": "eslint --ext .jsx,.js ./src/",
    "test:lint": "npm run test:csslint && npm run test:eslint",
    "test:unused": "depcheck --ignores stylelint-config-standard,webpack-cli",
    "start": "node src/server/index.js",
    "start:dbg": "node inspect src/server/index.js",
    "start:dev": "webpack-dev-server"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js",
      "<rootDir>/test-setup.js"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/char-lie/pattern-recognition-server.git"
  },
  "author": "char-lie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/char-lie/pattern-recognition-server/issues"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "https://github.com/char-lie/pattern-recognition-server#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "depcheck": "^0.8.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.1.1",
    "jest-enzyme": "^7.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm": "^6.11.2",
    "npm-check-updates": "^3.1.21",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "ws": "^7.1.2"
  }
}
