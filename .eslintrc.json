{
  "extends": "airbnb",
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": "warn",
    "no-console": "error",
    "no-underscore-dangle": ["error", {
        "allowAfterThis": true
    }]
  }
}
